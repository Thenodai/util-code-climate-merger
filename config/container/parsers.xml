<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="service.parser.checkstyle_parser"
                 class="Paysera\Component\CodeClimateMerger\Parser\CheckstyleParser">
            <tag name="parser" format="checkstyle" />
        </service>
        <service id="service.parser.parser_registry"
                 class="Paysera\Component\CodeClimateMerger\Parser\ParserRegistry">
        </service>
    </services>
</container>
